{% for mnd, dager in stat_mnd %}
	google.charts.setOnLoadCallback(pameldte_per_dag_per_maned_{{mnd}});
	function pameldte_per_dag_per_maned_{{mnd}}() {
		var data = google.visualization.arrayToDataTable([
			['Sesong', {% for ar in stat_ar %}'{{ ar }}'{{ loop.last ? '' : ','}}{% endfor %}],
				{% for dag in 1..dager %}
					['{{dag}}.{{mnd|maned}}', {% for ar in stat_ar %}{{ stat[ ar~'-'~mnd~'-'~(dag<10?'0'~dag:dag) ] is defined ? stat[ ar~'-'~mnd~'-'~(dag<10?'0'~dag:dag) ] : 0 }}{{ not loop.last ? ', ' : '' }}{% endfor %}]{{ not loop.last ? ',
' : ''}}{% endfor %}{{ not loop.last ? ',
' : ''}}
		]);
		
		var options = {
			chartArea: {width: '100%'},
			title: 'Antall påmeldte per dag i {{mnd|maned}}',
			curveType: 'function',
			legend: { position: 'bottom' }
		};
		
		var chart = new google.visualization.LineChart(document.getElementById('pameldte_per_dag_per_maned_{{mnd}}'));
		chart.draw(data, options);
	}
	
	
	
	
	
	
	
	
	
