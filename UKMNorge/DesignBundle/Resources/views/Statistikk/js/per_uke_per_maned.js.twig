{% for mnd, dager in stat_mnd %}
	google.charts.setOnLoadCallback(pameldte_per_uke_per_mnd_{{mnd}});
	
	function pameldte_per_uke_per_mnd_{{mnd}}() {
		var data = google.visualization.arrayToDataTable([
	['Sesong', {% for ar in stat_ar %}'{{ ar }}'{{ loop.last ? '' : ','}}{% endfor %}],
			{% for uke_navn, uke_ar in uker[ mnd ] %}
				['{{ uke_navn ~' '~ mnd|maned }}', {% for ar in stat_ar %}{{ uker[ mnd ][ uke_navn ][ ar ] is defined ? uker[ mnd ][ uke_navn ][ ar ] : 0 }}{{ loop.last ? '' : ', '}}{% endfor %}]{{ loop.last ? '' : ',
'}}{% endfor %},
		]);
		
		var options = {
			chartArea: {width: '100%'},
			title: 'Antall p√•meldte per uke {{mnd|maned}}',
			curveType: 'function',
			legend: { position: 'bottom' }
		};
		
		var chart = new google.visualization.LineChart(document.getElementById('pameldte_per_uke_per_mnd_{{mnd}}'));
		
		chart.draw(data, options);
	}
{% endfor %}