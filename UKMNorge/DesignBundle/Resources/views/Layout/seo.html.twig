<link rel="canonical" href="{{SEO.canonical}}" />

<meta name="description" content="{{ SEO.description }}">
<meta name="author" content="{{ SEO.author }}">
<meta property="fb:app_id" content="141016739323676" />
<meta property="og:site_name" content="{{SEO.site_name}}" />
<meta property="og:type" content="{{SEO.type}}" />
<meta property="og:title" content="{{SEO.title|raw}}"/>
<meta property="article:section" content="{{SEO.section}}" />

{% if SEO.image_url is defined %}
	<meta property="og:image" content="{{SEO.image_url}}" />
	<meta property="og:width" content="{{SEO.image_width}}" />
	<meta property="og:height" content="{{SEO.image_height}}" />
{% elseif SEO.image is defined %}
	<meta property="og:image" content="{{SEO.image}}" />
{% endif %}

<meta property="og:url" content="{{SEO.canonical}}" />
<meta property="og:description" content="{{SEO.description}}" />

{% if SEO.type == 'article' %}
	<meta property="article:published_time" content="{{SEO.published_time}}" />
	<meta property="article:modified_time" content="{{SEO.modified_time}}" />
	<meta property="article:author" content="{{SEO.author}}" />
{% endif %}

<meta property="og:locale" content="nb_NO" />
<meta property="fb:admins" content="597605662" />
<meta name="google-site-verification" content="dD8sAwp7X8mpu2PN1iu4C3VWEDS8sMxYUavoVBrmnek" />