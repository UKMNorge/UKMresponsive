{% for logo in element.utgaver %}
	<div class="header-line mt-4 mb-2">{{ logo.navn }}</div>
	<img class="float-right ml-2 mb-2 p-2 {{ logo.class }}" width="120" src="{{ logo.url }}" />

	{% for utgave in logo.filer %}
		<a href="{{ utgave.url }}" class="my-2 btn btn-outline-primary {{ not loop.first ? 'btn-sm' }}" target="_blank">
			{{ utgave.navn }}
		</a>
	{% endfor %}
	<div class="clearfix"></div>
{% endfor %}
